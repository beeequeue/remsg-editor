{
  "name": "remsg-inspector",
  "type": "module",
  "version": "0.0.0",
  "author": "BeeeQueue <adam@haglund.dev>",
  "license": "MIT",
  "private": true,
  "packageManager": "pnpm@10.7.1",
  "engines": {
    "node": "^22"
  },
  "simple-git-hooks": {
    "pre-commit": "pnpm lint-staged"
  },
  "lint-staged": {
    "*.{js,jsx,ts,tsx,json,json5,yaml,yml,css,scss,md}": [
      "prettier --write"
    ]
  },
  "scripts": {
    "build": "vite build",
    "dev": "vite",
    "lint": "eslint src",
    "test": "tsx --test src/**/*.test.ts",
    "typecheck": "svelte-check"
  },
  "resolutions": {
    "esbuild": "0.25.2"
  },
  "dependencies": {
    "remsg": "1.1.0",
    "svelte": "5.25.5",
    "svelte-multiselect": "11.0.0-rc.1"
  },
  "devDependencies": {
    "@antfu/eslint-config": "4.11.0",
    "@iconify-json/lucide": "1.2.34",
    "@iconify-json/simple-icons": "1.2.30",
    "@rollup/plugin-inject": "5.0.5",
    "@sveltejs/vite-plugin-svelte": "5.0.3",
    "@tsconfig/strictest": "2.0.5",
    "@tsconfig/svelte": "5.0.4",
    "@unocss/preset-icons": "65.5.0",
    "@unocss/preset-uno": "65.5.0",
    "@unocss/preset-web-fonts": "65.5.0",
    "@unocss/reset": "65.5.0",
    "@unocss/transformer-directives": "65.5.0",
    "@unocss/vite": "65.5.0",
    "buffer": "6.0.3",
    "eslint": "9.23.0",
    "eslint-plugin-svelte": "2.46.1",
    "lint-staged": "15.5.0",
    "prettier": "3.5.3",
    "prettier-plugin-svelte": "3.3.3",
    "prettier-plugin-tailwindcss": "0.6.11",
    "simple-git-hooks": "2.12.1",
    "svelte-check": "4.1.5",
    "typescript": "5.8.2",
    "vite": "6.2.4",
    "vite-plugin-pwa": "0.21.2"
  },
  "pnpm": {
    "overrides": {
      "@eslint/markdown": "-",
      "@stylistic/eslint-plugin": "-",
      "@vitest/eslint-plugin": "-",
      "eslint-plugin-jsonc": "-",
      "eslint-plugin-toml": "-",
      "eslint-plugin-vue": "-",
      "eslint-processor-vue-blocks": "-",
      "eslint-plugin-yml": "-",
      "jsonc-eslint-parser": "-",
      "toml-eslint-parser": "-",
      "yaml-eslint-parser": "-",
      "is-core-module": "npm:@nolyfill/is-core-module@^1",
      "safe-buffer": "npm:@nolyfill/safe-buffer@^1",
      "string.prototype.matchall": "npm:@nolyfill/string.prototype.matchall@^1"
    }
  }
}
